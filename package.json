{
  "name": "@svl-ad/hooks",
  "private": false,
  "version": "0.0.1",
  "keywords": [
    "React",
    "Components",
    "UI Design"
  ],
  "author": "ShiMu",
  "license": "MIT",
  "homepage": "https://svl-fe.github.io/hooks/",
  "repository": {
    "type": "git",
    "url": "https://github.com/svl-fe/hooks"
  },
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "files": [
    "dist",
    "es"
  ],
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "prepublishOnly": "npm run build"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "peerDependencies": {},
  "dependencies": {
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.178",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@umijs/fabric": "^2.8.1",
    "@umijs/test": "^3.0.5",
    "antd": "^4.18.4",
    "dumi": "^1.1.0",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "yorkie": "^2.0.0"
  }
}
